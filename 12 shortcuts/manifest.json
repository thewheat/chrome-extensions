{
  "name": "12 Shortcuts",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Shows examples of custom shortcuts",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Example Popup page",
    "default_popup": "popup.html"
  },
  "commands": {
    "toggle-feature-foo": {
      "description": "Toggle feature foo"
    },
    "toggle-feature-foo1": {
      "description": "Toggle feature foo1"
    },
    "toggle-feature-foo2": {
      "description": "Toggle feature foo2"
    }
  }
}
